            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_SQL_Lead</name>
        <label>Is SQL Lead</label>
        <locationX>182</locationX>
        <locationY>1019</locationY>
        <defaultConnector>
            <targetReference>Is_MQL_Lead</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Is_SQL_Lead_Yes</name>
