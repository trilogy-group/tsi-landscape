<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>High_Value_Opp__c</fullName>
    <description>Identify high-value opportunities.</description>
    <externalId>false</externalId>
    <formula>(Offer_ARR__c / CURRENCYRATE(Text( CurrencyIsoCode ))) &gt;= 100000 ||  (Current_ARR__c / CURRENCYRATE(Text( CurrencyIsoCode ))) &gt;= 100000</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Opportunity ARR &gt;= 100,000.  We use Offer ARR to exclude the effect of the Auto-Renewals at List.</inlineHelpText>
    <label>High Value Opp</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
