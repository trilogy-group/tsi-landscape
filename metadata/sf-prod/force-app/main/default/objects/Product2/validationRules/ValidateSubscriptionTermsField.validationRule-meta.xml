<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidateSubscriptionTermsField</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(Billing_Term__c, &quot;Yearly&quot;),  INCLUDES(Subscription_Terms__c,&quot;1&quot;))</errorConditionFormula>
    <errorMessage>Subscription term 0 not allowed for Yearly billing term</errorMessage>
</ValidationRule>
