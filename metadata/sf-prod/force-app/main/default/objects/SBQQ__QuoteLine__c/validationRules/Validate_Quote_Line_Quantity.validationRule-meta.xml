<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Quote_Line_Quantity</fullName>
    <active>false</active>
    <errorConditionFormula>SBQQ__Quantity__c &lt; SBQQ__Product__r.Min_Quantity__c
