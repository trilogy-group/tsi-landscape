<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>echosign_dev1__Signing_Deadline_Future</fullName>
    <active>true</active>
    <errorConditionFormula>AND( echosign_dev1__Signing_Deadline__c &lt; TODAY(),  TEXT(echosign_dev1__Status__c) = &quot;Draft&quot; )</errorConditionFormula>
    <errorMessage>The signing deadline date must not be in the past.</errorMessage>
</ValidationRule>
