                  "type": "object",
                  "value": {
                    "name": {
                      "type": "string",
                      "value": "quoteRecord"
                    },
                    "value": {
                      "type": "jsonpath",
                      "value": "$.steps.trigger.data.newRecord"
                    }
                  }
                }
              ]
            },
            "script": {
              "type": "string",
              "value": "exports.step = function(input, fileInput) {\n  const { quoteRecord } = input;\n  \n  // Retrieve the primary contact on the quote as it is automatically set at creation.\n  const contacts = Object.values(quoteRecord.sublists.contacts);\n  if (contacts.length > 0) {\n  \treturn contacts[0].id;\n  }\n  \n  // If there is no primary contact, return null.\n\treturn null;\n};"
            },
            "file_output": {
              "type": "boolean",
              "value": false
            }
          }
        },
